{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "pjctADF"
		},
		"AzureSqlDatabase1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase1'",
			"defaultValue": "Integrated Security=False;Encrypt=True;Connection Timeout=30;Data Source=gkcdrs.database.windows.net;Initial Catalog=gkcdrs;User ID=gkcdrs_admin"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://airlineadlsgen2.dfs.core.windows.net/"
		},
		"AzureDataLakeStorage1_properties_typeProperties_tenant": {
			"type": "string",
			"defaultValue": "bfde2954-db11-4a27-a6e6-c74cff05b265"
		},
		"AzureDataLakeStorage1_properties_typeProperties_servicePrincipalId": {
			"type": "string",
			"defaultValue": "5cacf149-1f1e-4b7c-94de-2ac82087d821"
		},
		"AzureDataLakeStorage_Sink_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datalakedevsink.dfs.core.windows.net/"
		},
		"AzureDataLakeStorage_Sink_properties_typeProperties_tenant": {
			"type": "string",
			"defaultValue": "bfde2954-db11-4a27-a6e6-c74cff05b265"
		},
		"AzureDataLakeStorage_Sink_properties_typeProperties_servicePrincipalId": {
			"type": "string",
			"defaultValue": "5cacf149-1f1e-4b7c-94de-2ac82087d821"
		},
		"AzureKeyVault1_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://sarankeyvault.vault.azure.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "pjctADF_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureSqlDatabase1_connectionString": {
						"value": "[parameters('AzureSqlDatabase1_connectionString')]"
					},
					"AzureKeyVault1_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault1_properties_typeProperties_baseUrl')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_tenant": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_tenant')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_servicePrincipalId": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_servicePrincipalId')]"
					},
					"AzureDataLakeStorage_Sink_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage_Sink_properties_typeProperties_url')]"
					},
					"AzureDataLakeStorage_Sink_properties_typeProperties_tenant": {
						"value": "[parameters('AzureDataLakeStorage_Sink_properties_typeProperties_tenant')]"
					},
					"AzureDataLakeStorage_Sink_properties_typeProperties_servicePrincipalId": {
						"value": "[parameters('AzureDataLakeStorage_Sink_properties_typeProperties_servicePrincipalId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}